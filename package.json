{
  "name": "statistics-app",
  "version": "0.1.0",
  "homepage": "http://snury.github.io/statistics-app",
  "private": true,
  "scripts": {
    "lint:js": "esw src",
    "lint:js:fix": "esw --fix src",
    "lint:js:watch": "esw -w src",
    "test": "NODE_PATH=src CI=true react-scripts test --env=jsdom",
    "test:watch": "NODE_PATH=src react-scripts test --env=jsdom",
    "precommit": "npm run test && npm run && npm run lint:js",
    "dev": "NODE_PATH=src react-scripts start & node ./server/app.js",
    "build": "NODE_PATH=src react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.3",
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-easy-chart": "^1.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "custom-react-scripts": "^0.2.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "gh-pages": "^2.0.0",
    "jest-localstorage-mock": "^2.1.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.3.0"
  }
}
